[ui]
ssh = ssh -C
username = My name <me@example.com>
merge = kdiff3

[merge-tools]
kdiff3.args=--auto -L1 base --L2 local --L3 other $base $local $other -o $output
kdiff3.regkey=Software\KDiff3
kdiff3.regappend=\kdiff3.exe
kdiff3.fixeol=True
kdiff3.gui=True

[extensions]
color = 
rebase =
histedit =
progress =
transplant =
pager =
mq =

[diff]
git = 1
showfunc = 1
unified = 8

[defaults]
commit = -v

